{
  "name": "claude-api-proxy",
  "version": "1.0.0",
  "description": "TypeScript script that converts Anthropic API requests to OpenAI format",
  "type": "module",
  "main": "src/index.ts",
  "scripts": {
    "dev": "bun run --watch src/entrypoints/bun.ts",
    "build": "bun build src/entrypoints/bun.ts --outdir dist --target bun",
    "start": "bun run dist/bun.js",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "convert": "bun run src/cli.ts"
  },
  "keywords": ["anthropic", "openai", "api", "proxy", "converter"],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "zod": "^3.22.4",
    "hono": "^4.4.12",
    "@hono/node-server": "^1.11.3"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "bun-types": "latest"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  }
}
