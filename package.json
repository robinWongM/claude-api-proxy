{
  "name": "claude-api-proxy",
  "version": "1.0.0",
  "description": "TypeScript script that converts Anthropic API requests to OpenAI format",
  "type": "module",
  "main": "src/index.ts",
  "scripts": {
    "dev": "bun run --watch src/entrypoints/bun.ts",
    "build": "bun build src/entrypoints/bun.ts --outdir dist --target bun",
    "start": "bun run dist/bun.js",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "lint": "biome check src",
    "lint:fix": "biome check --write src",
    "format": "biome format --write src"
  },
  "keywords": [
    "anthropic",
    "openai",
    "api",
    "proxy",
    "converter"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "@ai-sdk/deepseek": "^1.0.7",
    "@ai-sdk/openai": "^2.0.10",
    "@ai-sdk/openai-compatible": "^1.0.8",
    "@ai-sdk/provider": "^2.0.0",
    "@hono/node-server": "^1.11.3",
    "@hono/zod-validator": "^0.7.2",
    "ai": "^5.0.10",
    "hono": "^4.4.12",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@anthropic-ai/sdk": "^0.60.0",
    "@biomejs/biome": "^2.1.4",
    "@types/bun": "latest",
    "bun-types": "latest",
    "typescript": "^5.9.2"
  }
}
